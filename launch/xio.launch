<launch>
    <arg name="use_sim_time" value="true"/>
    <node name="imuPre" pkg="XIO" type="imuPreintegration_node" output="screen"/>
    <node pkg="rosbag" type="play" name="playbag1" output="screen" args="--clock $(find XIO)/bags/1.bag"/>
  <node name="rvizvisualisation" pkg="rviz" type="rviz" output="screen" args="-d $(find XIO)/cfg/xio.rviz" />
</launch>